  <property name="styleSheet">
   <string notr="true">/* Main Application Styling */
QWidget {
    font-family: &quot;Segoe UI&quot;, &quot;Arial&quot;, sans-serif;
    font-size: 13px;
    color: #3a3a3a;
}

/* Main Window Background */
QMainWindow {
    background-color: #f8f5ff;  /* Very light purple background */
}

/* Toolbar Styling */
QToolBar {
    background-color: #e6e0ff;
    border: none;
    border-bottom: 1px solid #d5c8ff;
    padding: 4px;
    spacing: 6px;
}

QToolButton {
    background-color: transparent;
    border: 1px solid transparent;
    border-radius: 5px;
    padding: 5px 8px;
    color: #5a4d8a;
}

QToolButton:hover {
    background-color: #d5c8ff;
    border: 1px solid #b8a3ff;
}

QToolButton:pressed {
    background-color: #b8a3ff;
    color: white;
}

QToolButton:checked {
    background-color: #9e86ff;
    color: white;
}

/* Menu Bar Styling */
QMenuBar {
    background-color: #f0ebff;
    border-bottom: 1px solid #d5c8ff;
    padding: 3px;
}

QMenuBar::item {
    padding: 5px 12px;
    background-color: transparent;
    color: #5a4d8a;
}

QMenuBar::item:selected {
    background-color: #d5c8ff;
    border-radius: 4px;
}

QMenu {
    background-color: #ffffff;
    border: 1px solid #d5c8ff;
    border-radius: 6px;
    padding: 8px;
    color: #5a4d8a;
}

QMenu::item {
    padding: 6px 24px 6px 12px;
    background-color: transparent;
}

QMenu::item:selected {
    background-color: #f0ebff;
    border-radius: 4px;
}

QMenu::separator {
    height: 1px;
    background-color: #e6e0ff;
    margin: 4px 8px;
}

/* Status Bar Styling */
QStatusBar {
    background-color: #e6e0ff;
    color: #5a4d8a;
    border-top: 1px solid #d5c8ff;
    padding: 4px 8px;
}

/* Push Buttons */
QPushButton {
    background-color: #e6e0ff;
    border: 1px solid #d5c8ff;
    border-radius: 6px;
    padding: 7px 14px;
    color: #5a4d8a;
    min-width: 80px;
}

QPushButton:hover {
    background-color: #d5c8ff;
    border-color: #b8a3ff;
}

QPushButton:pressed {
    background-color: #9e86ff;
    color: white;
}

QPushButton:disabled {
    background-color: #f0f0f0;
    color: #aaaaaa;
    border-color: #dddddd;
}

/* Primary Action Button */
QPushButton[primary=&quot;true&quot;] {
    background-color: #9e86ff;
    border: 1px solid #8a6dff;
    color: white;
}

QPushButton[primary=&quot;true&quot;]:hover {
    background-color: #8a6dff;
}

QPushButton[primary=&quot;true&quot;]:pressed {
    background-color: #7a5bef;
}

/* Checkboxes and Radio Buttons */
QCheckBox, QRadioButton {
    color: #5a4d8a;
    spacing: 6px;
}

QCheckBox::indicator, QRadioButton::indicator {
    width: 18px;
    height: 18px;
}

QCheckBox::indicator {
    border: 1px solid #b8a3ff;
    border-radius: 4px;
    background-color: white;
}

QCheckBox::indicator:checked {
    background-color: #9e86ff;
    image: url(:/icons/check_white.svg);
}

QRadioButton::indicator {
    border: 1px solid #b8a3ff;
    border-radius: 9px;
    background-color: white;
}

QRadioButton::indicator:checked {
    background-color: #9e86ff;
    border: 5px solid white;
}

/* Line Edits and Text Edits */
QLineEdit, QTextEdit, QPlainTextEdit {
    background-color: white;
    border: 1px solid #d5c8ff;
    border-radius: 6px;
    padding: 5px 5px;
    selection-background-color: #9e86ff;
    selection-color: white;
}

QLineEdit:focus, QTextEdit:focus, QPlainTextEdit:focus {
    border: 1px solid #9e86ff;
}

/* Combo Boxes */
QComboBox {
    background-color: white;
    border: 1px solid #d5c8ff;
    border-radius: 6px;
    padding: 3px 5px;
    min-width: 6em;
    color: #5a4d8a;
}

QComboBox:hover {
    border-color: #b8a3ff;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 24px;
    border-left: 1px solid #d5c8ff;
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
}

QComboBox::down-arrow {
    image: url(:/icons/down_arrow_purple.svg);
    width: 12px;
    height: 12px;
}

QComboBox QAbstractItemView {
    background-color: white;
    border: 1px solid #d5c8ff;
    border-radius: 6px;
    padding: 6px 0;
    selection-background-color: #f0ebff;
    selection-color: #5a4d8a;
    outline: none;
}

/* Sliders */
QSlider::groove:horizontal {
    height: 6px;
    background: #e6e0ff;
    border-radius: 3px;
}

QSlider::handle:horizontal {
    background: #9e86ff;
    width: 18px;
    margin: -6px 0;
    border-radius: 9px;
    border: 2px solid white;
}

/* Progress Bars */
QProgressBar {
    border: 1px solid #d5c8ff;
    border-radius: 6px;
    text-align: center;
    background-color: white;
    color: #5a4d8a;
}

QProgressBar::chunk {
    background-color: #9e86ff;
    border-radius: 5px;
}

/* Group Boxes */
QGroupBox {
    border: 1px solid #d5c8ff;
    border-radius: 8px;
    margin-top: 12px;
    padding-top: 20px;
    background-color: white;
    color: #5a4d8a;
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 12px;
    padding: 0 6px;
    color: #7a5bef;
}

/* Tab Widgets */
QTabWidget::pane {
    border: 1px solid #d5c8ff;
    border-radius: 8px;
    background: white;
    margin-top: 4px;
}

QTabBar::tab {
    background: #e6e0ff;
    border: 1px solid #d5c8ff;
    border-bottom: none;
    padding: 8px 16px;
    color: #5a4d8a;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
    margin-right: 2px;
}

QTabBar::tab:selected {
    background: white;
    color: #7a5bef;
    border-bottom: 1px solid white;
    margin-bottom: -1px;
}

QTabBar::tab:hover {
    background: #d5c8ff;
}

/* Scroll Bars */
QScrollBar:vertical {
    background: #f0ebff;
    width: 12px;
    border-radius: 6px;
}

QScrollBar::handle:vertical {
    background: #d5c8ff;
    min-height: 30px;
    border-radius: 6px;
}

QScrollBar::handle:vertical:hover {
    background: #b8a3ff;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0;
    background: none;
}

/* Table View Styling */
QTableView {
    gridline-color: #e6e0ff;
    background-color: white;
    alternate-background-color: #faf8ff;
    border: 1px solid #d5c8ff;
    border-radius: 5px;
    selection-background-color: #f0ebff;
    selection-color: #5a4d8a;
}

QTableView QTableCornerButton::section {
    background-color: #f0ebff;
    border: 1px solid #d5c8ff;
    border-top-left-radius: 7px;
}

QHeaderView::section {
    background-color: #f0ebff;
    color: #5a4d8a;
    padding: 8px;
    border: none;
    border-bottom: 1px solid #d5c8ff;
    font-weight: 500;
}

QHeaderView::section:checked {
    background-color: #d5c8ff;
    color: #5a4d8a;
}

QTableView::item {
    padding: 2px;
}

QTableView::item:selected {
    background-color: #f0ebff;
}

QTableView::item:hover {
    background-color: #f5f2ff;
}

/* Tree View Styling */
QTreeView {
    background-color: white;
    border: 1px solid #d5c8ff;
    border-radius: 8px;
    alternate-background-color: #faf8ff;
    show-decoration-selected: 1;
}

QTreeView::item {
    padding: 6px;
    border: 1px solid transparent;
}

QTreeView::item:hover {
    background-color: #f5f2ff;
}

QTreeView::item:selected {
    background-color: #f0ebff;
    color: #5a4d8a;
}

QTreeView::branch {
    image: url(:/icons/branch_closed_purple.svg);
}

QTreeView::branch:open {
    image: url(:/icons/branch_open_purple.svg);
}

/* Splitter Styling */
QSplitter::handle {
    background-color: #e6e0ff;
    width: 6px;
    height: 6px;
}

QSplitter::handle:hover {
    background-color: #d5c8ff;
}

/* Dock Widget Styling */
QDockWidget {
    titlebar-close-icon: url(:/icons/close_purple.svg);
    titlebar-normal-icon: url(:/icons/float_purple.svg);
    background: white;
    border: 1px solid #d5c8ff;
    border-radius: 8px;
}

QDockWidget::title {
    background: #f0ebff;
    padding: 8px;
    text-align: center;
    color: #5a4d8a;
    border-top-left-radius: 7px;
    border-top-right-radius: 7px;
}

/* Spin Boxes */
QSpinBox, QDoubleSpinBox {
    background-color: white;
    border: 1px solid #d5c8ff;
    border-radius: 6px;
    padding: 6px;
    color: #5a4d8a;
}

QSpinBox::up-button, QDoubleSpinBox::up-button {
    border-left: 1px solid #d5c8ff;
    border-bottom: 1px solid #d5c8ff;
    border-top-right-radius: 5px;
    width: 20px;
}

QSpinBox::down-button, QDoubleSpinBox::down-button {
    border-left: 1px solid #d5c8ff;
    border-bottom-right-radius: 5px;
    width: 20px;
}

QSpinBox::up-arrow, QDoubleSpinBox::up-arrow {
    image: url(:/icons/up_arrow_purple.svg);
}

QSpinBox::down-arrow, QDoubleSpinBox::down-arrow {
    image: url(:/icons/down_arrow_purple.svg);
}

/* Tooltips */
QToolTip {
    background-color: #ffffff;
    color: #5a4d8a;
    border: 1px solid #d5c8ff;
    border-radius: 6px;
    padding: 8px;
}

/* QListView Styling - Purple Theme */
QListView {
    background-color: white;
    border: 1px solid #d5c8ff;
    border-radius: 8px;
    padding: 4px;
    outline: 0; /* Remove focus border */
    color: #5a4d8a;
    alternate-background-color: #faf8ff;
    show-decoration-selected: 1;
}

/* List Items */
QListView::item {
    height: 32px;
    padding: 6px 12px;
    border-bottom: 1px solid #f0ebff;
}

QListView::item:hover {
    background-color: #f5f2ff;
    border-radius: 4px;
}

QListView::item:selected {
    background-color: #f0ebff;
    color: #7a5bef;
    border-radius: 4px;
    border: none;
}

QListView::item:selected:active {
    background-color: #e6e0ff;
}

/* Disabled Items */
QListView::item:disabled {
    color: #b8a3ff;
    background-color: transparent;
}

/* Scrollbar (consistent with table view) */
QListView QScrollBar:vertical {
    background: #f0ebff;
    width: 12px;
    border-radius: 6px;
    margin: 2px;
}

QListView QScrollBar::handle:vertical {
    background: #d5c8ff;
    min-height: 30px;
    border-radius: 6px;
}

QListView QScrollBar::handle:vertical:hover {
    background: #b8a3ff;
}

/* Special Cases */
QListView[iconMode=&quot;true&quot;]::item {
    padding: 8px;
    border-radius: 6px;
    border: 1px solid transparent;
}

QListView[iconMode=&quot;true&quot;]::item:hover {
    background-color: #f5f2ff;
    border: 1px solid #e6e0ff;
}

QListView[iconMode=&quot;true&quot;]::item:selected {
    background-color: #f0ebff;
    border: 1px solid #d5c8ff;
}

/* Drag and Drop Visuals */
QListView::item[dragging=&quot;true&quot;] {
    background-color: #e6e0ff;
    border: 1px dashed #b8a3ff;
    color: #7a5bef;
}</string>
